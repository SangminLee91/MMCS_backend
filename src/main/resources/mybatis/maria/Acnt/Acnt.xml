<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="acnt">



    <select id="acnt.account_total_cnt" parameterType="hashmap" resultType="hashmap">
        call usp_account_total_cnt_s(#{auth_cd}, #{dept_cd});
    </select>
    <select id="account_list" parameterType="hashmap" resultType="hashmap">
        call usp_account_list_s(#{auth_cd}, #{dept_cd}, #{page, jdbcType=INTEGER}, #{start, jdbcType=INTEGER}, #{limit, jdbcType=INTEGER});
    </select>


</mapper>